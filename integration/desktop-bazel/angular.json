{
  "$schema": "../../node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "app": {
      "root": "src",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "targets": {
        "build": {
          "builder": "../../bazel-bin/packages/devkit/npm:build",
          "options": {
            "outputPath": "dist",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "showCircularDependencies": false
          }
        },
        "serve": {
          "builder": "../../bazel-bin/packages/devkit/npm:serve",
          "options": {
            "buildTarget": "app:build"
          }
        }
      }
    }
  }
}
